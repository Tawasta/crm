<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id="partner_contact_tree_view" model="ir.ui.view">
			<field name="name">contact_tree_view</field>
			<field name="model">res.partner</field>
			<field eval="30" name="priority"/>
			<field name="arch" type="xml">
       	   		<tree string="Contacts" default_order='parent_id'>
                    <field name='parent_id' />
               		<field name='name' />
               		<field name='email' />
               		<field name='phone' />

                    <field name='street' />
                    <field name='zip' />
                    <field name='city' />

                    <field name='last_salesperson' />
              	</tree>

			</field>
		</record> 

	</data>
</openerp>